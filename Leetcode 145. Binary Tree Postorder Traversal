class Solution {
public:
  void preorder(TreeNode* root,vector<int> &nodes){
    if(root==NULL) return;
    preorder(root->left,nodes);
     preorder(root->right,nodes);
    nodes.push_back(root->val);
   
  }
    vector<int> postorderTraversal(TreeNode* root) {
        vector<int> nodes;
        preorder(root,nodes);
        return nodes;
    }
};
